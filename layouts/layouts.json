{
	"tracker_right_map": {
		"type": "container",
		"background": "#212121",
		"content": {
			"type": "dock",
			"dropshadow": true,
			"content": [
				{
					"type": "dock",
					"dock": "left",
					"v_alignment": "stretch",
					"margin": "0,0,0,0",
					"content": [
						{
							"type": "group",
							"header": "Abilities, Jewels, & Treasures",
							"dock": "top",
							"content": {
								"type": "layout",
								"key": "item_grid"
							}
						},
						{
							"type": "group",
							"header": "YAML Options",
							"dock": "top",
							"content": {
								"type": "layout",
								"key": "ap_options"
							}
						},
						{
							"type": "group",
							"header": "Required Jewels",
							"dock": "top",
							"content": {
								"type": "layout",
								"key": "initial_jewels"
							}
						},
						{
							"type": "group",
							"header": "Difficulty",
							"dock": "top",
							"content": {
								"type": "layout",
								"key": "settings"
							}
						}
					]
				},
				{
					"type": "dock",
					"content": [
						{
							"type": "tabbed",
							"dock": "top",
							"tabs": [
								{
									"title": "Map",
									"icon": "images/map.png",
									"content": {
										"type": "map",
										"maps": ["map"]
									}
								}
							]
						}
					]
				}
			]
		}
	},
	"tracker_top_map": {
		"type": "container",
		"background": "#212121",
		"content": {
			"type": "dock",
			"content": [
				{
					"type": "dock",
					"dock": "bottom",
					"content": [
						{
							"type": "group",
							"header": "Items",
							"dock": "left",
							"content": {
								"type": "layout",
								"key": "item_grid_wide"
							}
						},
						{
							"type": "group",
							"header": "Pinned Locations",
							"content": {
								"style": "wrap",
								"type": "recentpins",
								"orientation": "horizontal",
								"compact": true
							}
						}
					]
				},
				{
					"type": "map",
					"maps": ["map"]
				}
			]
		}
	}
}
